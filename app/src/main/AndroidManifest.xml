<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.holamundo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HolaMundo">
        <activity
            android:name=".CartActivity"
            android:exported="true" />
        <activity
            android:name=".SuggestionsActivity"
            android:exported="true" />
        <activity android:name=".SucursalActivity" />
        <activity android:name=".DishesActivity" />
        <activity android:name=".AddDishActivity" />
        <activity android:name=".PrincipalActivity" />
        <activity android:name=".AvisoPrivacidadActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".RegistroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>